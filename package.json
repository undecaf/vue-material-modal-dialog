{
  "name": "vue-material-modal-dialog",
  "version": "0.3.0",
  "description": "A reusable modal dialog for Vue Material",
  "keywords": [
    "vue",
    "component",
    "material",
    "modal",
    "dialog"
  ],
  "author": "F. Kasper <fkasper@modus-operandi.at>",
  "license": "MIT",
  "main": "dist/components.ssr.js",
  "module": "dist/components.esm.js",
  "unpkg": "dist/components.min.js",
  "style": "dist/components.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/undecaf/vue-material-modal-dialog"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "prebuild": "rimraf dist/components.*",
    "build": "NODE_ENV=production rollup --config rollup.config.js",
    "postbuild": "node terser.js dist/components.*.js",
    "prebuild:example": "rimraf docs/example",
    "build:example": "vue-cli-service build --dest docs/example",
    "serve": "vue-cli-service serve",
    "lint": "eslint '{src,tests}/**/*.{js,vue}'",
    "lint:fix": "npm run lint -- --fix",
    "test:unit": "karma start",
    "test:unit:debug": "karma start --browsers Chromium --no-single-run",
    "test:e2e": "testcafe"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@vue/cli": "^4.5.12",
    "@vue/cli-service": "~4.5.12",
    "@vue/test-utils": "^1.1.4",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "eslint": "^6.8.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^6.2.2",
    "karma": "^6.3.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^4.0.2",
    "material-icons-font": "^2.1.0",
    "mocha": "^8.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.45.2",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-license": "^2.3.0",
    "rollup-plugin-vue": "^5.1.9",
    "source-map-loader": "^1.1.3",
    "testcafe": "^1.14.0",
    "typeface-roboto": "1.1.13",
    "vue": "^2.6.12",
    "vue-material": "^1.0.0-beta-15",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.8.2"
  },
  "peerDependencies": {
    "vue": "^2.6.12",
    "vue-material": ">1.0.0-beta"
  }
}
